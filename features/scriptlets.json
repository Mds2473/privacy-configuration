{
    "_meta": {
        "description": "Run Adguard scriptlets."
    },
    "state": "enabled",
    "exceptions": [],
    "settings": {
        "scriptlets": [
            {
                "name": "trustedSetAttr",
                "attrs": {
                    "selector": "a",
                    "attr": "href",
                    "value": "#"
                }
            },
            {
                "name": "setCookie",
                "attrs": {
                    "domain": "example.com",
                    "name": "test",
                    "value": "accepted"
                }
            },
            {
                "name": "removeCookie",
                "attrs": {
                    "domain": "example.com",
                    "match": "bloop"
                }
            },
            {
                "name": "setConstant",
                "attrs": {
                    "property": "test",
                    "value": "no"
                }
            },
            {
                "name": "trustedReplaceNodeText",
                "attrs": {
                    "nodeName": "a",
                    "textMatch": "test",
                    "pattern": "test",
                    "replacement": "test2",
                    "extraArgs": [
                        "verbose"
                    ]
                }
            },
            {
                "name": "abortCurrentInlineScript",
                "attrs": {
                    "property": "test",
                    "search": "test"
                }
            },
            {
                "name": "abortOnPropertyRead",
                "attrs": {
                    "property": "test"
                }
            },
            {
                "name": "abortOnPropertyWrite",
                "attrs": {
                    "property": "test"
                }
            }
        ]
    }
}
