{
    "_meta": {
        "description": "Run Adguard scriptlets."
    },
    "state": "enabled",
    "exceptions": [],
    "settings": {
        "scriptlets": [
            {
                "name": "trustedSetAttr",
                "attrs": {
                    "selector": "a",
                    "attr": "href",
                    "value": "#"
                }
            },
            {
                "name": "setCookie",
                "attrs": {
                    "domain": "example.com",
                    "name": "test",
                    "value": "accepted"
                }
            },
            {
                "name": "removeCookie",
                "attrs": {
                    "domain": "example.com",
                    "match": "test"
                }
            }
        ]
    }
}
